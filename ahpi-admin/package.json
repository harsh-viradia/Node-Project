{
  "name": "LMS-Admin-Panel",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:base-css": "tailwindcss build ./styles/globals.css -o ./styles/base.css",
    "dev": "next -p 5689",
    "build": "next build",
    "export": "next build && next export",
    "start": "next start -p 7086",
    "start-docker": "next start -p 3000",
    "start-qa": "next start -p 7011",
    "start-prod": "next start -p 7046",
    "prettier-lint": "prettier --write *.{js,jsx,css,less,md}",
    "eslint-lint": "eslint --cache --fix *.{js,jsx}",
    "init": "yarn install && npm run build && pm2 start npm --name '7086-ahpi-admin' -- start",
    "init-qa": "yarn install && npm run build && pm2 start npm --name '7011-ahpi-admin-panel' -- run 'start-qa'",
    "init-prod": "yarn install && npm run build && pm2 start npm --name '7046-ahpi-admin-panel' -- run 'start-prod'",
    "serve": "yarn install && npm run build && pm2 reload '7086-ahpi-admin'",
    "serve-qa": "yarn install && npm run build && pm2 reload '7011-ahpi-admin-panel'",
    "serve-prod": "yarn install && npm run build && pm2 reload '7046-ahpi-admin-panel'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.5",
    "@headlessui/react": "^1.4.2",
    "@hookform/resolvers": "^2.8.8",
    "@knovator/api": "^0.0.10",
    "@knovator/can": "^1.0.2",
    "@knovator/file-uploader": "git+ssh://git@git.knovator.in:knovators/file-uploader.git",
    "@knovator/masters-admin": "^2.1.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "axios": "^0.27.2",
    "cookies-next": "^2.1.1",
    "css-tooltip": "^0.3.4",
    "dashjs": "^2.9.3",
    "date-fns": "^2.28.0",
    "dayjs": "^1.10.8",
    "highcharts": "^10.2.0",
    "highcharts-react-official": "^3.1.0",
    "hls.js": "0.12.4",
    "iron-session": "^6.0.5",
    "moment-timezone": "^0.5.34",
    "next": "12.0.7",
    "qs": "^6.10.3",
    "react": "17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-collapsed": "^3.3.2",
    "react-css-transition-replace": "^4.0.5",
    "react-date-range": "^1.4.0",
    "react-dom": "17.0.2",
    "react-dropzone": "^12.0.4",
    "react-hook-form": "^7.24.1",
    "react-hot-toast": "^2.2.0",
    "react-html-parser": "^2.0.2",
    "react-image-crop": "^10.0.4",
    "react-otp-input": "^2.4.0",
    "react-pro-sidebar": "0.7.1",
    "react-query": "^3.34.16",
    "react-quill": "2.0.0-beta.2",
    "react-router-dom": "^6.2.1",
    "react-select": "^5.2.2",
    "react-slick": "^0.28.1",
    "react-star-ratings": "^2.3.0",
    "react-table": "^7.7.0",
    "react-tabs": "^3.2.3",
    "react-time-picker-input": "^2.0.10",
    "react-tiny-popover": "^7.1.0",
    "react-tooltip": "^4.2.21",
    "simple-crypto-js": "^3.0.1",
    "use-debounce": "^7.0.1",
    "video-react": "0.15.0",
    "yup": "^0.32.11"
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "npm run eslint-lint",
      "prettier --write --ignore-unknown",
      "npx cspell '**'"
    ],
    "*.+(json|css|md)": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged && yarn build",
      "pre-push": "yarn build"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.7",
    "@next/eslint-plugin-next": "^12.0.7",
    "autoprefixer": "10.4.0",
    "cspell": "^5.13.4",
    "eslint": "^8.5.0",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-jsconfig": "^1.1.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "eslint-plugin-write-good-comments": "^0.1.3",
    "eslint-plugin-yml": "^0.12.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.3",
    "postcss": "8.4.4",
    "prettier": "^2.5.1",
    "tailwindcss": "3.0.0"
  }
}
